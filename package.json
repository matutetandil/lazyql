{
  "name": "lazyql",
  "version": "0.3.0",
  "description": "A lightweight library that makes GraphQL resolvers truly lazy by default",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src --ext .ts",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "graphql",
    "lazy",
    "resolver",
    "apollo",
    "nestjs",
    "performance",
    "optimization"
  ],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/matutetandil/lazyql.git"
  },
  "bugs": {
    "url": "https://github.com/matutetandil/lazyql/issues"
  },
  "homepage": "https://github.com/matutetandil/lazyql#readme",
  "engines": {
    "node": ">=18"
  },
  "peerDependencies": {
    "reflect-metadata": "^0.1.13 || ^0.2.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "eslint": "^8.55.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "tsup": "^8.0.1",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0",
    "reflect-metadata": "^0.2.1"
  }
}
